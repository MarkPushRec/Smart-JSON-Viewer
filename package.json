{
  "name": "smart-json-viewer",
  "version": "1.0.0",
  "description": "Dark mode JSON viewer",
  "main": "main.js",
  "scripts": {
    "react-start": "cross-env NODE_ENV=development webpack-dev-server --config webpack.config.js",
    "react-build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "electron-start": "electron-forge start",
    "start": "concurrently \"npm:react-start\" \"wait-on http://localhost:3000 && npm:electron-start\"",
    "package": "npm run react-build && electron-forge package",
    "make": "npm run react-build && electron-forge make"
  },
  "dependencies": {
    "@electron/remote": "^2.1.2",
    "electron": "^28.0.0",
    "electron-is-dev": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-json-view": "^1.21.3"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "@babel/preset-react": "^7.23.3",
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "style-loader": "^3.3.3",
    "wait-on": "^7.2.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "smart_json_viewer"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}